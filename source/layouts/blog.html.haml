- wrap_layout :boilerplate do
  .show-for-medium-up
    = partial 'layouts/header'
  .show-for-small-only
    = partial 'layouts/small_header'
  %section#main
    .row
      %h1
        = current_page.data.title
    .row
      .columns.large-12.large-centered
        %article
          = yield
  = partial 'layouts/footer'
